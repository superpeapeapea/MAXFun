Listen 11220
<VirtualHost *:11220>                                     
  ServerName d.cathod.tv
  ServerAlias *d.cathod.tv
  DocumentRoot /cathod/cathoddev/app/storage/logs/
  <Directory /cathod/cathoddev/app/storage/logs/ >
    Options +Indexes FollowSymLinks
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

</VirtualHost>

<VirtualHost *:443>
SSLEngine On
SSLCertificateFile /cathod/cathoddev/configs/ssl/cathod.crt
SSLCertificateKeyFile /cathod/cathoddev/configs/ssl/cathod.key
  ServerName cathod.tv
 ServerAlias *cathod.tv
  DocumentRoot /cathod/cathoddev/public/
  <Directory /cathod/cathoddev/public/ >
    Options Indexes FollowSymLinks
    AllowOverride All

    Order allow,deny
    Allow from all
  </Directory>

</VirtualHost>

<VirtualHost *:80>
#SSLEngine On                                                                                                
#SSLCertificateFile /cathod/cathoddev/configs/ssl/cathod.crt                                                 
#SSLCertificateKeyFile /cathod/cathoddev/configs/ssl/cathod.key                                              
  ServerName cathod.tv
 ServerAlias *cathod.tv
  DocumentRoot /cathod/cathoddev/public/
  <Directory /cathod/cathoddev/public/ >
    Options Indexes FollowSymLinks
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

</VirtualHost>

<VirtualHost *:10029>
SSLEngine On
SSLCertificateFile /cathod/cathoddev/configs/ssl/cathod.crt
SSLCertificateKeyFile /cathod/cathoddev/configs/ssl/cathod.key
  DocumentRoot /cathod/cathoddev/public/
  <Directory /cathod/cathoddev/public/ >
    Options Indexes FollowSymLinks
    AllowOverride All
    Require all granted
    #Order allow,deny
    #Allow from all
  </Directory>

</VirtualHost>
